<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tw.sunny.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity"
            android:label="畢專一路發">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:label="@string/text_login"></activity>

        <activity android:name=".ForgetPasswordActivity"
            android:label="忘記密碼"></activity>

        <activity android:name=".RegisterActivity"
            android:label="註冊"></activity>

        <activity android:name=".MainActivity"
            android:label="主畫面"></activity>

        <activity android:name=".ShootActivity"
            android:label="拍照主畫面"></activity>

        <activity android:name=".ShootCodeActivity"
            android:label="拍照資訊"></activity>


    </application>

</manifest>
